@model IEnumerable<Aydinturk_agency.Models.ApplicationUser>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div class="container p-3">


    <div class="row p-4">
        <div class="col-6">
            <h2>ادارة الحسابات</h2>
        </div>
        <div class="col-6 text-end">
            <a asp-controller="Account" asp-area="Identity" asp-action="Register" class="btn btn-primary">
              <i class="bi bi-plus-square"></i> &nbsp; تسجيل مشترك جديد
            </a>
        </div>
    </div>
    <br /> <br /> <br />
    <table class= "table table-striped table-bordered" id="accountsTable" style="width:100%">
        <thead>
            <tr>
                <th>اسم الحساب</th>
                <th>العنوان</th>
                <th>الرصيد</th>
                <th>رقم الهاتف</th>
                <th>البريد الالكتروني</th>
                <th></th>
            </tr>
        </thead>

        <tbody>
           
        </tbody>
   </table>

</div>

@section Scripts {
    <script src="~/js/accounts.js"></script>
}